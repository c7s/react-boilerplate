{
    "name": "@react-boilerplate/frontend",
    "version": "0.0.0",
    "author": "c7s",
    "private": true,
    "scripts": {
        "prepare": "node ./tools/install/create-symlinks && cd .. && yarn i",
        "i": "yarn install --frozen-lockfile",
        "a": "yarn add -E -D",
        "r": "yarn remove",
        "start": "yarn i && yarn start:no-install",
        "start:no-install": "node ./tools/start/open-browser & concurrently -k \"node ./tools/start/start-dev-server\" \"yarn generate:apollo --watch\"",
        "build": "yarn i && yarn build:no-install",
        "build:final": "yarn build && node ./tools/build/remove-dev-files",
        "build:no-install": "webpack --env.build",
        "build:final:no-install": "yarn build:no-install && node ./tools/build/remove-dev-files",
        "analyze": "yarn i && webpack --env.build --env.analyze",
        "prettier:all": "prettier --write \"**/*.{js,ts,tsx,json,graphql,md,*rc}\"",
        "generate:apollo": "node ./tools/apollo/schema && node ./tools/apollo/config && yarn generate:apollo-types",
        "generate:apollo-types": "node ./tools/apollo/types",
        "lint:js": "eslint .",
        "lint:ts": "tslint -t codeFrame --project tsconfig.json ./{src,@types,config}/**/*.{ts,tsx} && tsc --project tsconfig.json",
        "lint:css": "stylelint ./src/**/*.{ts,tsx}",
        "pre-commit": "lint-staged"
    },
    "engines": {
        "node": "8.11.1",
        "yarn": "^1.6.0"
    },
    "lint-staged": {
        "*.js": [
            "eslint --fix",
            "git add"
        ],
        "*.{ts,tsx}": [
            "node ./tools/git/hooks/check-types",
            "tslint -t codeFrame --project tsconfig.json --fix",
            "git add"
        ],
        "./src/**/*.{tsx,ts}": [
            "stylelint"
        ],
        "*.{js,ts,tsx,json,graphql,md,*rc}": [
            "prettier --write",
            "git add"
        ],
        "{package.json,yarn.lock}": [
            "node ./tools/git/hooks/check-yarn-lock",
            "node ./tools/git/hooks/check-package-json"
        ]
    },
    "devDependencies": {
        "@babel/core": "7.0.1",
